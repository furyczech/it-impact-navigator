{
  "name": "it-impact-navigator-scripts",
  "version": "1.0.0",
  "description": "Scripts for managing IT Impact Navigator data",
  "main": "setupSupabase.ts",
  "scripts": {
    "setup": "ts-node setupSupabase.ts",
    "upload": "ts-node uploadToSupabase.ts",
    "generate-sql": "ts-node generateSQL.ts",
    "migrate": "ts-node runMigrations.ts",
    "verify": "ts-node verifyData.ts",
    "upload-dependencies": "ts-node uploadDependencies.ts",
    "direct-upload": "ts-node directUpload.ts",
    "test-db-connection": "ts-node -e \"import { testConnection } from './directDb'; testConnection().then(connected => process.exit(connected ? 0 : 1));\"",
    "upload-via-rest": "ts-node uploadViaRest.ts",
    "inspect-table": "ts-node inspectTable.ts",
    "test-db": "ts-node testDbOperations.ts",
    "inspect-schema": "ts-node inspectTableSchema.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@types/pg": "^8.15.5",
    "dotenv": "^16.0.3",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
